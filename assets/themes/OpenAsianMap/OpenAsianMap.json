{
	"id": "asianmap",
	"title": {
		"en": "Open Asian map"
	},
	"shortDescription": {
		"en": "A map to find Asian related places."
	},
	"description": {
		"en": "Searching for a Chinese restaurant? Want to make your own shushi? Or what about a relaxing walk in a Japanese garden? Find any Asian related place closeby with the help of this map."
	},
	"language": [
		"en"
	],
	"maintainer": "",
	"icon": "./logo.svg",
	"version": "0",
	"startLat": 0,
	"startLon": 0,
	"startZoom": 1,
	"widenFactor": 0.05,
	"socialImage": "",
	"layers": [
		{
			"id": "asianshops",
			"name": {
				"en": "Asian shops"
			},
			"minzoom": 10,
			"overpassTags": {
				"and": [
					"shop~*",
					{
						"or": [
							"cuisine=asian",
							"cuisine=chinese",
							"cuisine=indian",
							"cuisine=japanese",
							"cuisine=korean",
							"cuisine=thai",
							"origin=asian",
							"origin=chinese",
							"origin=indian",
							"origin=japanese",
							"origin=korean",
							"origin=thai"
						]
					}
				]
			},
			"title": {
				"render": {
					"en": "Asian shop"
				},
				"mappings": [
					{
						"if": {
							"and": [
								"name~*"
							]
						},
						"then": {
							"en": "{name}"
						}
					}
				]
			},
			"description": {
				"en": "This layer shows all Asian shops."
			},
			"tagRenderings": [
				{
					"question": {
						"en": "What is the name of this shop?"
					},
					"render": {
						"en": "{name}"
					},
					"freeform": {
						"key": "name"
					},
					"id": "shops-name",
					"condition": {
						"and": [
							"name="
						]
					}
				},
				{
					"render": {
						"en": "Shop: {shop}"
					},
					"question": {
						"en": "What type of shop is this?"
					},
					"freeform": {
						"key": "shop"
					},
					"mappings": [
						{
							"if": {
								"and": [
									"shop=convenience"
								]
							},
							"then": {
								"en": "Convenience store"
							}
						},
						{
							"if": {
								"and": [
									"shop=supermarket"
								]
							},
							"then": {
								"en": "Supermarket"
							}
						},
						{
							"if": {
								"and": [
									"shop=greengrocer"
								]
							},
							"then": {
								"en": "Fruit and vegetables"
							}
						},
						{
							"if": {
								"and": [
									"shop=bakery"
								]
							},
							"then": {
								"en": "Bakery"
							}
						},
						{
							"if": {
								"and": [
									"shop=seafood"
								]
							},
							"then": {
								"en": "Seafood"
							}
						}
					],
					"id": "shops-shop"
				},
				"website",
				"email",
				"phone",
				"opening_hours",
				"payment-options",
				"reviews",
				"images"
			],
			"hideUnderlayingFeaturesMinPercentage": 0,
			"presets": [
				{
					"tags": [
						"shop=yes",
						"origin=asian"
					],
					"title": {
						"en": "Asian shop"
					},
					"description": {
						"en": "Add a new Asian shop"
					}
				}
			],
			"mapRendering": [
				{
					"icon": {
						"render": "https://wiki.openstreetmap.org/w/images/9/96/Convenience-14.svg",
						"mappings": [
							{
								"if": "shop=supermarket",
								"then": "https://wiki.openstreetmap.org/w/images/7/76/Supermarket-14.svg"
							}
						]
					},
					"iconBadges": [
						{
							"if": "opening_hours~*",
							"then": "isOpen"
						}
					],
					"label": {
						"mappings": [
							{
								"if": "name~*",
								"then": "<div style='background: white; padding: 0.25em; border-radius:0.5em'>{name}</div>"
							}
						]
					},
					"location": [
						"point",
						"centroid"
					]
				}
			],
			"color": {
				"render": "#00f"
			},
			"allowMove": true,
			"wayHandling": 1,
			"icon": {
				"freeform": {
					"addExtraTags": []
				},
				"mappings": []
			},
			"iconSize": {
				"freeform": {
					"addExtraTags": []
				},
				"mappings": []
			},
			"width": {
				"freeform": {
					"addExtraTags": []
				},
				"mappings": []
			}
		},
