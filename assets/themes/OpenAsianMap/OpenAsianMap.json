{
	"id": "asianmap",
	"title": {
		"en": "Open Asian map"
	},
	"shortDescription": {
		"en": "A map to find Asian related places."
	},
	"description": {
		"en": "Searching for a Chinese restaurant? Want to make your own shushi and need ingredients? Or what about a relaxing walk in a Japanese garden? Find all Asian related places around with the help of this map."
	},
	"language": [
		"en"
	],
	"maintainer": "",
	"icon": "./logo.svg",
	"version": "0",
	"startLat": 0,
	"startLon": 0,
	"startZoom": 1,
	"widenFactor": 0.05,
	"socialImage": "",
	"defaultBackgroundId": "CartoDB.Voyager",
	"layers": [
		{
			"id": "asianshops",
			"name": {
				"en": "Asian shops"
			},
			"minzoom": 10,
			"overpassTags": {
				"and": [
					"shop~*",
					"shop!~vacant",
					"disused:shop=",
					{
						"or": [
							"cuisine~(.*;)?asian(;.*)?",
							"cuisine~(.*;)?chinese",
							"cuisine~(.*;)?indian(;.*)?",
							"cuisine~(.*;)?japanese(;.*)?",
							"cuisine~(.*;)?korean(;.*)?",
							"cuisine~(.*;)?thai(;.*)?",
							"origin~(.*;)?asian(;.*)?",
							"origin~(.*;)?chinese(;.*)?",
							"origin~(.*;)?indian(;.*)?",
							"origin~(.*;)?japanese(;.*)?",
							"origin~(.*;)?korean(;.*)?",
							"origin~(.*;)?thai(;.*)?"
						]
					}
				]
			},
			"title": {
				"render": {
					"en": "Asian shop"
				},
				"mappings": [
					{
						"if": {
							"and": [
								"name~*"
							]
						},
						"then": {
							"en": "{name}"
						}
					}
				]
			},
			"description": {
				"en": "This layer shows all Asian shops."
			},
			"tagRenderings": [
				{
					"group": "General",
					"question": {
						"en": "What is the name of this shop?"
					},
					"render": {
						"en": "<b>Name: </b>{name}"
					},
					"freeform": {
						"key": "name"
					},
					"id": "shops-name"
				},
				{
					"group": "General",
					"render": {
						"en": "<b>Type: </b>{shop}"
					},
					"question": {
						"en": "What type of shop is this?"
					},
					"freeform": {
						"key": "shop"
					},
					"mappings": [
						{
							"if": {
								"and": [
									"shop=convenience"
								]
							},
							"then": {
								"en": "<b>Type: </b>Convenience store"
							}
						},
						{
							"if": {
								"and": [
									"shop=supermarket"
								]
							},
							"then": {
								"en": "<b>Type: </b>Supermarket"
							}
						},
						{
							"if": {
								"and": [
									"shop=greengrocer"
								]
							},
							"then": {
								"en": "<b>Type: </b>Fruit and vegetables"
							}
						},
						{
							"if": {
								"and": [
									"shop=bakery"
								]
							},
							"then": {
								"en": "<b>Type: </b>Bakery"
							}
						},
						{
							"if": {
								"and": [
									"shop=seafood"
								]
							},
							"then": {
								"en": "<b>Type: </b>Seafood"
							}
						}
					],
					"id": "shops-type"
				},
				{
					"builtin": "website",
					"override": {
						"group": "contact",
						"render": "<b>Website: </b><a href='{website}' target='_blank'>{website}</a>"
					}
				},
				{
					"builtin": "email",
					"override": {
						"group": "contact",
						"render": "<b>Email: </b><a href='mailto:{email}' target='_blank'>{email}</a>"
					}
				},
				{
					"builtin": "phone",
					"override": {
						"group": "contact",
						"render": "<b>Phone: </b><a href='tel:{phone}'>{phone}</a>"
					}
				},
				{
					"builtin": "payment-options",
					"override": {
						"group": "extra"
					}
				},
				{
					"builtin": "opening_hours",
					"override": {
						"group": "extra"
					}
				},
				{
					"builtin": "reviews",
					"override": {
						"group": "extra"
					}
				},
				{
					"builtin": "images",
					"override": {
						"group": "extra"
					}
